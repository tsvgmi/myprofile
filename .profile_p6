export P6_HOME=/opt/etrade/p6
export P6_BUILD=yes
export JAVA_HOME=/etrade/pkgs/linux/intel/sunjava2/1.5.0_06
export PATH=$P6_HOME/bin:$JAVA_HOME/bin:$UAT_INGR_ROOT/bin:$PATH
export CVSROOT=:pserver:cvsserver.etrade.com:2401/etrade/cvsserver/cvs

# yum aliases
alias yumgold='sudo yum -c http://yum.etrade.com/proxy/p6.rhas3.x86/dev-main-gold/yum.conf'
alias yumtest='sudo yum -c http://yum.etrade.com/proxy/p6.rhas3.x86/dev-main-test/yum.conf'
alias yumbeta='sudo yum -c http://yum.etrade.com/proxy/p6.rhas3.x86/dev-main-beta/yum.conf'

# AF and Web dev related
export AF_SRC_ROOT=$HOME/work
export AF_BUILD_FROM_JARS=true 
export AF_ROOT=$AF_SRC_ROOT/af
export AF_HOME=$AF_SRC_ROOT/af
export DETUS_HOME=$AF_SRC_ROOT/det-us
export AF_REPO_REMOTE=http://act.etrade.com:9000/af-repo
export AF_REPO_LOCAL=$AF_SRC_ROOT/af-repo
export AF_BUILD_FROM_JARS=true
export AF_REPO_DEPOT=$AF_SRC_ROOT/af-repo-remote
export THIRDPARTY_REPOSITORY=$AF_SRC_ROOT/3rdparty-repo

# Service related modules
export DETUS_HOME=$AF_SRC_ROOT/det-us
export SVC_HOME=$HOME/work
export LD_LIBRARY_PATH=${P6_HOME}/lib:$LD_LIBRARY_PATH

